
# HTTP sites
map $http_preread_server_name $internal_port {
  my-http-site.loc            8080;
  my-site.loc                 8010;
  my-site2.loc                8011;
  default                     8019;
}

# HTTPS (SSL) sites
map $ssl_preread_server_name $ssl_internal_port {
  my-https-site.loc           44380;
  my-site.loc                 44310;
  my-site2.loc                44311;
  default                     44319;
}
